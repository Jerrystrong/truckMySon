                                <!-- <label class="text-slate-700">Jeremie KIMPIOKA</label> -->
                                <input type="tel" class="text-slate-700 focus:border-0 focus:outline-0 placeholder:text-slate-700/60 text-[12px]" placeholder="Entrez le portable de l'enseignant" name="teacherPhone">
