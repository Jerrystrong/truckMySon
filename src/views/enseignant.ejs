                                        {"Identifiant": "<%= teachers[i].teacherIdentifiant %>"},
                                        {"Nom": "<%= teachers[i].teacherName %>"},
                                        {"Post-nom": "<%= teachers[i].teacherPostname %>"},
                                        {"Prenom": "<%= teachers[i].teacherLastname %>"},
                                        {"Age": "<%= teachers[i].teacherBirthDay %> ans"},
                                        {"Classe": "<%= teachers[i].teacherClasse %>"},
                                        {"Identifiant classe": "<%= teachers[i].teacherClasseIdentifiant %>"},
                                        {"E-mail": "<%= teachers[i].teacherEmail %>"},
                                        {"Tel": "<%= teachers[i].teacherPhone %>"}
                                        ])'>
                                        <td class="px-4 py-2"><%= teachers[i].teacherIdentifiant %></td>
                                        <td class="px-4 py-2"><%= teachers[i].teacherName %></td>
                                        <td class="px-4 py-2"><%= teachers[i].teacherPostname %></td>
                                        <td class="px-4 py-2"><%= teachers[i].teacherLastname %></td>
                                        <td class="px-4 py-2"><%= teachers[i].teacherBirthDay %> ans</td>
                                        <td class="px-4 py-2"><%= teachers[i].teacherClasse %></td>
                                        <td class="px-4 py-2"><%= teachers[i].teacherClasseIdentifiant %></td>
                                        <td class="px-4 py-2"><%= teachers[i].teacherPhone %></td>
                                        <td class="px-4 py-2"><%= teachers[i].teacherEmail %></td>
                                    </tr>
                                <% } %>
                            </tbody>
                        </table>
                    </div>

                 </div>
            </div>
        </div>
        <!-- <%- include('footer')-%> -->
        <script>
            const getClasses=async()=>{
                const teacherClasse=document.getElementById('teacherClasse')
                const fetchD= await fetch('/get-classes')
                const json= await fetchD.json()
                try{
                    json.data.forEach(field => {
                        teacherClasse.innerHTML=teacherClasse.innerHTML+`<option value="${field.name}">${field.name}</option>` 
                    });
                }catch(err){
                    console.error(error)
                }
            }
            getClasses()
            // events
            // add user component
            const addIUser=document.querySelector('#addIUser')
            addIUser.addEventListener('submit',async(e)=>{
                e.preventDefault()
                console.log(e.currentTarget)
                const successTeacher=document.querySelector('#successTeacher')
                const successTeacherCont=document.querySelector('#successTeacherCont')
                console.log(addIUser.value)
                const formData = new FormData(addIUser);
                const formObject = Object.fromEntries(formData.entries());

                const fetchD = await fetch('/add/teacher', {
                    method: 'POST',
                    body: JSON.stringify(formObject),
                    headers: {
                        'Content-Type': 'application/json'
                    }
                });
                const data= await fetchD.json()
                if(data.success){
                    successTeacher.classList.remove('hidden')
                    successTeacherCont.innerHTML=`
                        <h1 class="text-xl mb-4">Enregistrement reussi</h1>
                        <p class="text-[12px] text-slate-700/50">Enseignant ajouter avec success</p>
                        <button class="bg-red-500/70 px-3 hover:bg-red-500 text-slate-50 rouded-md p-1 rounded-full cursor-pointer" id="closeSuccess" onclick="removeSuccess()">Fermer</button>
                    `
                }else{
                    successTeacher.classList.remove('hidden')
                    successTeacherCont.innerHTML=`
                        <h1 class="text-xl mb-4">Erreur d'enregistrement</h1>
                        <p class="text-[12px] text-red-700/50">${data.message}</p>
                        <button class="bg-red-500/70 px-3 hover:bg-red-500 text-slate-50 rouded-md p-1 rounded-full cursor-pointer" id="closeSuccess" onclick="removeSuccess()">Fermer</button>
                    `
                }
            })
            const removeSuccess=()=>{
                const closeSuccess=document.querySelector('#successTeacher')
                const addTeacherEl=document.getElementById('addTeacher')
                closeSuccess.classList.add('hidden')
                addTeacherEl.classList.add('hidden')
            }
        </script>
    </body>
</html>